<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated aas -->
      <category
            id="de.dfki.cos.basys.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.dfki.cos.basys.aas.presentation.AasModelWizardID"
            name="%_UI_AasModelWizard_label"
            class="de.dfki.cos.basys.aas.presentation.AasModelWizard"
            category="de.dfki.cos.basys.Wizard.category.ID"
            icon="icons/full/obj16/AasModelFile.gif">
