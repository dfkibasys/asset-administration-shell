      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated aas -->
      <editor
            id="de.dfki.cos.basys.aas.presentation.AasEditorID"
            name="%_UI_AasEditor_label"
            icon="icons/full/obj16/AasModelFile.gif"
            extensions="aas"
            class="de.dfki.cos.basys.aas.presentation.AasEditor"
            contributorClass="de.dfki.cos.basys.aas.presentation.AasActionBarContributor">
